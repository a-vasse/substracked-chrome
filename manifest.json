{
  "name": "Substracked Extension",
  "version": "1.0",
  "description": "Add your subscription here!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.amazon.co.jp/-/en/gp/primecentral"
      ],
      "js": ["scripts/scrape-amazon.js"]
    }
  ]
}
